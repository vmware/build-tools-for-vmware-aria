    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "strict": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "outDir": "dist"
