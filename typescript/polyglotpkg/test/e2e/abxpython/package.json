{
	"id": "",
  "name": "abx-python",
	"version": "1.0.10-SNAPSHOT",
	"description": "Autogenerated python action",
	"private": false,
	"scripts": {
		"build": "polyglotpkg --env abx",
		"clean": "run-script-os || rm -Rf out dist tmp",
		"clean:win32": "rmdir /S /Q out dist tmp || echo Nothing to clean",
		"clean:default": "rm -Rf out dist tmp",
		"test": "echo \"Error: no test specified\""
	},
	"keywords": [
		"polyglot",
		"package",
		"abx",
		"python"
	],
	"author": "VMware WWCoE",
	"license": "VMware Confidential",
	"devDependencies": {
		"@types/node": "^14.0.13",
		"@vmware-pscoe/polyglotpkg": "latest",
		"@vmware-pscoe/vropkg": "latest",
		"run-script-os": "^1.1.6",
		"sinon": "^11.1.2",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.5"
	},
	"platform": {
		"runtime": "python",
		"action": "abxpython",
		"tags": [],
		"entrypoint": "handler.handler",
		"base": "out",
		"timeoutSeconds": 900,
		"memoryInMB": 300
	},
	"abx": {
		"shared": true,
		"inputSecrets": [],
		"inputConstants": [],
		"inputs": {
			"tagKey": ""
		}
	},
	"files": [
		"src",
		"out",
		"*.js",
		"!package.json"
	]
}
