{
    "name": "@vmware-pscoe/vrotsc",
    "version": "4.10.1-SNAPSHOT",
    "author": "VMware PSCoE",
    "license": "VMware Confidential",
    "description": "vRO TypeScript to JavaScript transpiler",
    "keywords": [
        "vRO",
        "TypeScript",
        "compiler",
        "language",
        "javascript"
    ],
    "main": "./lib/vrotsc.js",
    "bin": {
        "vrotsc": "bin/vrotsc"
    },
    "dependencies": {
        "minimatch": "^10.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "28.0.1",
        "@rollup/plugin-node-resolve": "15.3.0",
        "@rollup/plugin-terser": "0.4.4",
        "@types/fs-extra": "^11.0.4",
        "@types/gulp": "4.0.17",
        "@types/js-yaml": "4.0.9",
        "@types/minimist": "1.2.5",
        "@types/node": "^22.10.1",
        "@types/xmldoc": "1.1.9",
        "ansi-colors": "4.1.3",
        "fs-extra": "11.2.0",
        "glob": "11.0.0",
        "gulp": "5.0.0",
        "gulp-help": "1.6.1",
        "gulp-typescript": "6.0.0-alpha.1",
        "is-keyword-js": "1.0.3",
        "js-yaml": "4.1.0",
        "merge2": "1.4.1",
        "minimatch": "10.0.1",
        "minimist": "1.2.8",
        "prettier": "^3.5.3",
        "rollup": "4.28.0",
        "source-map": "^0.7.4",
        "ts-node": "10.9.2",
        "typescript": "5.7.2",
        "uuid": "11.0.3",
        "vinyl-buffer": "1.0.1",
        "vinyl-source-stream": "2.0.0",
        "xmlbuilder": "15.1.1",
        "xmldoc": "1.3.0"
    },
    "scripts": {
        "build": "gulp build",
        "compile": "gulp compile",
        "clean": "gulp clean",
        "format": "prettier --write 'lib/**/*.js'"
    },
    "engines": {
        "node": ">=22.13.0 <23.0.0",
        "npm": ">=10.9.2"
    },
    "browser": {
        "fs": false,
        "os": false,
        "path": false,
        "crypto": false,
        "buffer": false,
        "@microsoft/typescript-etw": false,
        "source-map-support": false,
        "inspector": false
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "printWidth": 80
    }
}
