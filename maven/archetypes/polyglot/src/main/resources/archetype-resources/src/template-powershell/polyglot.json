{
  "platform": {
#if ( $type == "vro" )
    "runtime": "powercli:13-powershell-7.4",
#else
    "runtime": "powershell",
#end
    "environment": "",
    "action": "auto",
    "tags": [],
    "entrypoint": "out/handler.handler"
  },
#if ( $type == "vro" )
  "vro": {
    "module": "${groupId}",
    "inputs": {
      "limit": "number",
      "vraEndpoint": "CompositeType(host:string,base:string):VraEndpointType"
    },
    "outputType": "Array/string"
  },
#elseif ( $type == "abx" )
  "abx": {
    "inputs": {
      "limit": "",
      "vraEndpoint": ""
    }
  },
#end
  "files": ["%out", "!**/package.json", "!**/polyglot.json"]
}
